{% extends 'base.html.twig' %}

{% block body %}
    <div>
        Je vais organiser mes présentations ici (lesquelles affichées par tranches horaires, dans quel ordre, etc...)
    </div>
    <div>
        <ul id="sortable">
            {% for presentation in presentations %}
                <li class="ui-state-default" id="{{ presentation.id }}"><span class="ui-icon ui-icon-arrowthick-2-n-s"></span>{{ presentation.name }}</li>
            {% endfor %}
        </ul>
    </div>
    <div>
        <button id="getOrder">Get Order</button>
    </div>
{% endblock %}


